extends ../includes/template
block vars
  -var bodyClass='is-home';
block metatags
  title Effio
  meta(name="description", content="description")

block main
  article.slider
    section.slider__content
      h1.slider__title
      a.slider__btn(href="#") Más Información
      nav.slider__nav
    figure.slider__item
      img.slider__img
  section.section
    article.stock
      figure.stock__fig
      a.stock__btn(href="#") +
      h2.stock__title Activo Fijo
      ul.stock__list
        li.stock__list-item Toma de Inventario
        li.stock__list-item Identificación de los Bienes
        li.stock__list-item Conciliación Fisica y Rentable
        li.stock__list-item Resultados
