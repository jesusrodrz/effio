      p.about-us__content
      a.about-us__btn
        span.t-semibold Conósenos
      figure.about-us__video 
        iframe(src="https://player.vimeo.com/video/7396421" width="640" height="360" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen)
